
@model List<TechnicalService.Entity.Entities.Site.Slider>

<div class="home-carousel">

    <div class="dark-mask"></div>

    <div class="container">
        <div class="homepage owl-carousel">
@foreach (var item in Model)
{
            <div class="item">
                <div class="row">
                    <div class="col-sm-5 right">
                       
                        <h1>@item.Title</h1>
                        <p>
                            @item.Description
                        </p>
                    </div>
                    <div class="col-sm-7">
                        <img class="img-responsive" src="@item.PhotoUrl" alt="">
                    </div>
                </div>
            </div>
}
        </div>
        <!-- /.project owl-slider -->
    </div>
</div>
